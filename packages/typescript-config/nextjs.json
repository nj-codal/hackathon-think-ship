{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "jsx": "preserve",
        "noEmit": true,
        "plugins": [
            {
                "name": "next"
            }
        ]
    }
}